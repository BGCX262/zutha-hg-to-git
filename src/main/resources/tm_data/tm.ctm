# ==================== Topic Map Implementation Constructs ====================

# ASSOCIATION: ANONYMOUS TOPIC LINK
#--- An association between a ZDM reified association
#--- and an anonymous topic.
#--- Used for locating the anonymous topic when the ZDM association
#--- is deleted so the anonymous topic can also be deleted
ztm:anonymous-topic-link.

# TOPIC,ROLE: ANONYMOUS TOPIC
#--- A topic that doesn't exist in the ZDM.
#--- May be used for implementing ZDM concepts in TM friendly form
ztm:anonymous-topic.

# ROLE: REIFIED ZDM ASSOCIATION
ztm:reified-zdm-association.

# ==================== Core Item Types====================

# ------------------- Identity --------------------
#IDENTITY
zsi:identity zid:0MA; isa zsi:entity-type;
    has-trait(zsi:role,zid:0MB);
    kind-of(zsi:zutha-object,zid:0MC);

    name("Identity",zid:0MD);

    plays-role(zsi:identity,
        zsi:user-owns-identity,0,1,zid:0ME);
.

# ZUTHA IDENTITY
#---An authenticated agent that can perform actions on the Zutha knowledgebase
zsi:zutha-identity zid:0MF; isa zsi:entity-type;
    kind-of(zsi:identity,zid:0MG);

    name("Zutha Identity",zid:0MH);
.

# INDEPENDENT ZUTHA IDENTITY
#---A means of authentication that does not rely on a 3rd party such as a public key
zsi:independent-zutha-identity zid:0MJ; isa zsi:entity-type;
    kind-of(zsi:zutha-identity,zid:0MK);

    name("Independent Zutha Identity",zid:0ML);
.

# ASYMMETRIC KEY IDENTITY
zsi:asymmetric-key-identity zid:0MM; isa zsi:entity-type;
    kind-of(zsi:independent-zutha-identity,zid:0MN);

    name("Asymmetric Key Identity",zid:0MP);

    has-property(zsi:public-key,1,1,zid:0MQ);
    plays-role(zsi:asymmetric-key-identity,
        zsi:asymmetric-key-identity-uses-encryption-scheme,1,1,zid:0MR);
.

# DEPENDENT ZUTHA IDENTITY
#---A means of authentication that relies on a 3rd party to vouch for you on Zutha such as an OpenID or simple password
zsi:dependent-zutha-identity zid:0MT; isa zsi:entity-type;
    kind-of(zsi:zutha-identity,zid:0MU);

    name("Dependent Zutha Identity",zid:0MV);

    plays-role(zsi:identity,
        zsi:zutha-identity-provider-provides-identity,1,1,zid:0MW);
.

# OPENID IDENTITY
